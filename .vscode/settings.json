{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.trunk/*out": true,
    "**/.trunk": true,
    "**/.vscode": true,
    ".cargo": false,
    ".gitattributes": true,
    ".markdownlint.yaml": true,
    ".shellcheckrc": true,
    "Cargo.lock": false,
    ".gitignore": false
  },
  "explorerExclude.backup": {},
  "lldb.displayFormat": "auto",
  "lldb.showDisassembly": "always",
  "lldb.dereferencePointers": true,
  "lldb.consoleMode": "commands",
  "rust-analyzer.runnables.extraArgs": ["--features=dev"],
  "rust-analyzer.checkOnSave.allTargets": false,
  "rust-analyzer.checkOnSave.target": ["x86_64-unknown-linux-gnu"],
  "rust-analyzer.checkOnSave.extraArgs": ["--features=dev_linux"],
  "rust-analyzer.cargo.target": "x86_64-unknown-linux-gnu",
  "rust-analyzer.cargo.features": ["dev_linux"]
}

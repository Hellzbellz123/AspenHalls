  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Build and Debug 'Aspen Halls Native'",
      "cargo": {
        "args": [
          "build",
          "--bin=aspen_halls_native",
          "--package=aspen_halls_native"
        ],
        "filter": {
          "name": "aspen_halls_native",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}/launchers/native"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug 'Aspen Halls Native'",
      "cargo": {
        "args": [
          "run",
          "--bin=aspen_halls_native",
          "--package=aspen_halls_native"
        ],
        "filter": {
          "name": "aspen_halls_native",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}/launchers/native"
    }
  ]
}
